{
    "name": "jtlshop/vue-install",
    "description": "Vue.js based installer for Shop5",
    "version": "1.0.0",
    "keywords": ["jtl", "jtl-shop"],
    "homepage": "https://jtl-software.de/",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "JTL-Software GmbH",
            "email": "info@jtl-software.de",
            "homepage": "https://jtl-software.de/"
        }
    ],
    "require": {
        "php": ">=5.5.9",
        "fzaninotto/faker": "^1.6",
        "andyftw/image-faker": "^1.1",
        "imanee/imanee": "dev-master",
        "cocur/slugify": "^2.5",
        "jtlshop/shop4-wizard": "dev-master",
        "foxy/foxy": "^1.0"
    },
    "repositories": [
        { "type": "vcs", "url": "git@gitlab.jtl-software.de:f.moche/wizard.git" }
    ],
    "autoload": {
        "files": [ "VueInstaller.php", "DemoDataInstaller.php", "Faker/Commerce.php", "Faker/de_DE/Commerce.php"]
    },
    "config": {
        "foxy": {
            "manager": "npm",
            "manager-version": {
                "npm": ">=6.0"
            },
            "manager-install-options": " && npm audit fix && npm run build"
        }
    }
}
